{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.8.0",
            "memory": 2048,
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "^/public/(.*)",
            "dest": "/api/index.php"
        },
        {
            "src": "^/(.*)$",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://griya-asri.vercel.app/",
        "APP_NAME": "Kosku",
        "APP_KEY": "base64:70UnJ6hVJr3EY9JJDdy336ovApo24AOA5d5k05cW8g4=",
        "DB_CONNECTION": "sqlite",
        "CACHE_DRIVER": "array",
        "QUEUE_CONNECTION": "sync",
        "SESSION_DRIVER": "cookie",
        "LOG_CHANNEL": "stderr"
    },
    "outputDirectory": "kosku/public"
}